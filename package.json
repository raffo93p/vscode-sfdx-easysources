{
  "name": "sfvsc-easy-sources",
  "displayName": "sfvsc-easy-sources",
  "description": "A vscode extension to easily run commands for the plugin sfdx-easy-sources",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.showPanel2"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sfvsc-easy-sources.showPanel",
        "title": "SFDX EasySources: Show Panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all compile:*",
    "watch": "tsc -watch -p ./",
    "compile:extension": "tsc -p ./",
    "compile:views": "cd react-easysources && npm run build",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "start": "HOST='' && cd react-easysources && npm start"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "sfdx-easy-sources": "^0.5.0"
  }
}
